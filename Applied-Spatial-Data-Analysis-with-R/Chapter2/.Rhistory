library(raster)
install.packages("sp")
library(raster)
r <- raster("70042108.tif")
class(r)
inMemory(r)
object.size(r)
cellStats(r, max)
cellStats(r, min)
inMemory(r)
out <- raster(r)
all.equal(r, out)
bs <- blockSize(out)
bs
out <- writeStart(out, filename=tempfile(), overwrite=TRUE)
for (i in 1:bs$n) {
v <- getValues(r, row=bs$row[i], nrows=bs$nrows[i])
v[v <= 0] <- NA
writeValues(out, v, bs$row[i])
}
out <- writeStop(out)
cellStats(out, min)
cellStats(out, max)
inMemory(out)
plot(out, col=terrain.colors(100))
plot(auck_gshhs, add=TRUE)
plot(r, col=terrain.colors(100))
plot(out, col=terrain.colors(100))
r1 <- as(out, "SpatialGridDataFrame")
summary(r1)
r2 <- as(r1, "RasterLayer")
summary(r2)
